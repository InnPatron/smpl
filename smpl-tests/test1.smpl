fn if_basic() -> i32 {

	if true {
		return 0;
	}
}

fn if_default() -> i32 {

	if false {
		return 0;
	} elif false {
		return 1;
	}
	
	return 2;
}

fn if_empty() -> i32 {

	if false { } elif false { } elif true { } else { }

	return 100;
}

fn if_complex() -> i32 {

	if true {
		return 0;
	} elif false {
		return 1;
	} else {
		return 2;
	}
}
